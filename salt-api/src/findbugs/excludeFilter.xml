<?xml version="1.0" encoding="UTF-8"?>
<!-- False positive Filter for FindBugs -->
<FindBugsFilter>
  <Match>
    <Bug pattern="SE_NO_SERIALVERSIONID"/>
  </Match>
  <Match>
    <Bug pattern="RV_RETURN_VALUE_IGNORED_BAD_PRACTICE"/>
  </Match>
  <Match>
    <Class name="org.corpus_tools.salt.extensions.notification.Listener$Event"/>
    <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD"/>
  </Match>
  <Match>
    <Class name="org.corpus_tools.salt.extensions.notification.SaltNotificationFactory"/>
    <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD"/>
  </Match>
  <Match>
    <Class name="org.corpus_tools.salt.util.internal.persistence.SaltXML10Writer"/>
    <Bug pattern="OBL_UNSATISFIED_OBLIGATION"/>
  </Match>
</FindBugsFilter>
